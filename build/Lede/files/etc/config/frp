
config frp 'common'
	option log_max_days '3'
	option login_fail_exit '0'
	option enable_cpool '0'
	option time '40'
	option tcp_mux '1'
	option enabled '1'
	option vhost_http_port '80'
	option vhost_https_port '443'
	option server_addr '192.168.50.3'
	option server_port '7000'
	option token 'aptio'
	option log_level 'info'
	option enable_http_proxy '0'
	option protocol 'tcp'
	option tls_enable '0'
	option admin_enable '0'

config proxy
	option enable '1'
	option type 'https'
	option domain_type 'subdomain'
	option subdomain 'srs'
	option local_ip '192.168.50.5'
	option proxy_protocol_version 'disable'
	option use_encryption '1'
	option use_compression '1'
	option local_port '8000'
	option remark 'srs'

